<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('common/head'); %>
  <title><%= title %></title>
</head>
<body>
<%- include('common/header'); %>
  <div class="container">
    <div class="row">
      <div class="col-md-7">
        <form>
          <div class="modal-body">
              <div class="form-group">
                <label for="pageSignupName">用户名</label>
                <input class="form-control" id="pageSignupName" type="text" name="user[username]">
              </div>
              <div class="form-group">
                <label for="pageSignupPassword">密码</label>
                <input class="form-control" id="pageSignupPassword" type="password" name="user[password]">
              </div>
              <!-- 错误提示 -->
              <div class="alert alert-success page-success"> </div>
              <div class="alert alert-danger page-danger"></div>
          </div>
          <div class="modal-footer">
              <button type="button" class="btn btn-success" id="pageSignup">注册</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</body>
</html>